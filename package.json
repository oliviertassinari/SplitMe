{
  "name": "SplitMe",
  "version": "0.0.34",
  "description": "Split expenses with friends",
  "scripts": {
    "start": "npm run server:start",
    "server:development": "BABEL_DISABLE_CACHE=1 NODE_ENV=server.development node src/server/development.js",
    "server:staging": "NODE_ENV=production babel-node webpack.js --config=server.staging",
    "server:production": "NODE_ENV=production babel-node webpack.js --config=server.production",
    "server:start": "NODE_ENV=production node server/local/server.js",
    "server:imagemin": "imagemin server server",
    "pouchdb-server": "pouchdb-server --config pouchdb-server/config.json --dir pouchdb-server",
    "browser:development": "babel-node webpack.js --dev --config=browser.development",
    "browser:staging": "NODE_ENV=production babel-node webpack.js --config=browser.staging",
    "browser:production": "NODE_ENV=production babel-node webpack.js --config=browser.production",
    "cordova:icons": "babel-node cordova/generate-icons.js",
    "cordova:imagemin": "imagemin cordova/resources cordova/resources",
    "android:emulator": "emulator -avd Nexus_5_API_21_x86",
    "android:development": "NODE_ENV=android.development babel-node webpack.js --config=android.development && cd cordova && babel-node generate-config.js --config=android.development && cordova run android --debug",
    "android:prerelease": "NODE_ENV=production babel-node webpack.js --config=android.prerelease && cd cordova && babel-node generate-config.js --config=android.prerelease && cordova run android --device --release",
    "android:production": "NODE_ENV=production babel-node webpack.js --config=android.production && cd cordova && babel-node generate-config.js --config=android.production && cordova build android --release",
    "ios:development": "NODE_ENV=ios.development babel-node webpack.js --config=ios.development && cd cordova && babel-node generate-config.js --config=ios.development && cordova run ios --debug",
    "ios:prerelease": "NODE_ENV=production babel-node webpack.js --config=ios.prerelease && cd cordova && babel-node generate-config.js --config=ios.prerelease && cordova run ios --release",
    "ios:production": "NODE_ENV=production babel-node webpack.js --config=ios.production && cd cordova && babel-node generate-config.js --config=ios.production && cordova build ios --release",
    "test:unit": "BABEL_DISABLE_CACHE=1 NODE_ENV=test node test/index.js unit",
    "test:unit:watch": "BABEL_DISABLE_CACHE=1 NODE_ENV=test babel-node test/watch.js unit",
    "test:selenium": "selenium-standalone start",
    "test:e2e": "BABEL_DISABLE_CACHE=1 wdio webdriver.config.js",
    "test:i18n-health": "babel-node i18n-tasks.js",
    "test": "npm run lint && npm run test:unit && npm run test:i18n-health && npm run flow",
    "lint": "eslint src test *.js cordova/*.js icon/*.js && echo \"eslint: no lint errors\"",
    "lint:find-rules": "eslint-find-rules -u .eslintrc.js",
    "flow": "flow"
  },
  "private": true,
  "contributors": [
    {
      "name": "Olivier Tassinari",
      "email": "olivier.tassinari@gmail.com"
    }
  ],
